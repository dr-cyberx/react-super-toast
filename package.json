{
  "name": "react-super-toast",
  "version": "1.0.8",
  "main": "dist/my-library.umd.js",
  "module": "dist/my-library.es.js",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/my-library.es.js",
      "require": "./dist/my-library.umd.js"
    },
    "./types": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --project tsconfig.build.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "watch": "nodemon --watch src --ext ts,tsx --exec \"npm run build\""
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.3",
    "clsx": "^2.1.1",
    "framer-motion": "^12.6.3",
    "lucide-react": "^0.487.0",
    "nanoid": "^5.1.5",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^4.5.3"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "toast",
    "notification",
    "library",
    "ui"
  ],
  "author": "drcyberx aka Vishal Kumar",
  "description": "A simple and customizable toast notification library for React applications.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dr-cyberx/react-super-toast.git"
  },
  "homepage": "https://github.com/dr-cyberx/react-super-toast#readme",
  "bugs": {
    "url": "https://github.com/dr-cyberx/react-super-toast/issues"
  }
}
